{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
      "**/.git": true,
      "**/.gitattributes": true,
      "**/.gitignore": true,
      "**/.svn": true,
      "**/.DS_Store": true,
      "**/.editorconfig": true,
      "**/.vscode": true,
      "**/node_modules": true,
      "**/.quasar": true
  }
}
